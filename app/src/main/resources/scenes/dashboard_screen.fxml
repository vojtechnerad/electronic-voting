<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootPane" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.vse.java.elektronicke_volby.gui.DashboardController">
    <children>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" VBox.vgrow="NEVER">
            <buttons>
                <Label alignment="BASELINE_LEFT" text="Aktuálně přihlášený uživatel:" />
                <Label fx:id="labelName" text="labelForName" />
                <Label fx:id="labelBirthNumber" text="labelForBirthNumer" />
                <Button mnemonicParsing="false" onAction="#signOut" text="Odhlásit" />
            </buttons>
        </ButtonBar>
        <VBox fx:id="electionsDashboard" VBox.vgrow="ALWAYS" />
    </children>
</VBox>
